<h1><%= service.name %></h1>
<ul>
  <li>Description: <%= service.description %></li>
  <li>Version: <%= service.version %></li>
  <li>Data Source: <%= service.data_source %></li>
  <li>URL: <%= link_to service.url(base_url), service.url(base_url) %></li>
  <li>URLs: <% service.urls(base_url).each do |u| %> 
    <ul><li><%= link_to u, u %></li></ul>
    <% end %>
  </li>
</ul>
